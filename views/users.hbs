<div id="app">
    <v-list two-line>
        <v-list-tile v-for="(user, index) in users" :key="user.id" @click="onClick(user.id)">
            <v-list-tile-content>
                <v-list-tile-title v-html="user.name"></v-list-tile-title>
                <v-list-tile-sub-title v-html="user.email"></v-list-tile-sub-title>
            </v-list-tile-content>
        </v-list-tile>
    </v-list>
</div>
<script>
    new Vue({
        el: "#app",
        mounted() {
            console.log('mounted', this.users)
        },
        data() {
            return {
                users: {{{ users }}}
            }
        },
        methods : {
            onClick (id) {
                window.location.href = `/users/${id}/todos`
            }
        }
    })
</script>